/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=c0);
    Not(in=b,out=c1);
    And(a=c0,b=b,out=c00);
    And(a=a,b=c1,out=c01);
    Or(a=c00,b=c01,out=out);

}
